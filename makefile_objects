#=============================  Makefile Objects  =============================#
#
#	Lists the code components of Poseidon.
#
#=============================================================================#

#
#       Driver Code File = Main Program File, ONLY 1 FILE
#
CODE_drv = d.main_3D.o

#
#       External Code Files - Used by Driver Code only
#
CODE_ext = e.Driver_Params_Read.o              \
           e.SelfSimilar.o                      \
           e.HDF5_IO.o                          \
           e.CHIMERA_TEST_FUNCS.o               \
           e.CHIMERA_HDF5.o                     \
           e.Mesh_Module.o

#
#	Interface Code - Used to Link Poseidon with other projects
#
CODE_itf = i.Poseidon_Interface.o


#
#	Common Code Files - Used to define physcial constants and units
#
CODE_com = com.Poseidon_Constants.o              \
           com.Units_Module.o                    


#
#	Parameters File - Used to define set Parameters for the code
#
CODE_par = p.Poseidon_Parameters.o               \
           p.Driver_Parameters.o


#
#	Poseidon Code Files - Used to define the operations used in Poseidon
#
CODE_o  = z.Poseidon_Variables_Module.o          \
          z.Allocate_Module.o                    \
          z.Additional_Functions_Module.o        \
          z.Poseidon_Internal_Communication.o    \
          z.Jacobian_Internal_Functions.o        \
          z.IO_Functions_Module.o                \
          z.Poseidon_BC_Module.o                 \
	  z.CFA_3D_Master_Build.o	 	 \
          z.Poseidon_Petsc_Solver.o		 \
	  z.CFA_Newton_Raphson.o                 \
          z.Test_Functions_Module.o              \
	  z.Poseidon_MPI_Module.o                \
          z.Poseidon_Params_Read.o               \
          z.Poseidon_Main_Module.o               \
          z.Poseidon_Initialize_Module.o         \
          z.New_PETSc_Build_Module.o             \
          z.New_PETSc_Module.o


